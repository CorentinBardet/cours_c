#include<stdio.h>
#include <math.h>



typedef struct {
	int x;
	int y;
}point;



double calc_distance(point a, point b) {
	double distance = (b.x - a.x)*(b.x - a.x)+(b.y - a.y)*(b.y- a.y);
	distance = sqrt(distance);
	return distance;
}




int main(void){

	point a;
	a.x = 10;
	a.y = 10;

	point b;
	b.x = 20;
	b.y = 20;

	double distance = calc_distance(a, b);

	printf("%.2f", distance);




	return 0;
}
