int main(void){

int x_1;
int x_2;
int y_1;
int y_2;


int distance_x = x_2 - x_1;
int distance_y = y_2 - y_1;

distance_x = distance_x * distance_y;
distance_y = distance_x * distance_y;

int distance = distance_x = distance_y;
distance = sqrt(distance);

printf("%d", distance);




	return 0;
}
