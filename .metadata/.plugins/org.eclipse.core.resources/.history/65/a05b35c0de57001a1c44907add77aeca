#include<stdio.h>
#include <math.h>

int calc_dist(int distance_x, int distance_y) {
	distance_x = distance_x * distance_x;
	distance_y = distance_y * distance_y;
	int distance = distance_x + distance_y;
	distance = sqrt(distance);
	return distance;
}

/*int x_point1;
