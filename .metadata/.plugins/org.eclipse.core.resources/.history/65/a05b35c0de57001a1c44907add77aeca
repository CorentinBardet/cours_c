int main(void){

int x_1 = 20;
int x_2 = 35;
int y_1 = 45;
int y_2 = 70;


int distance_x = x_2 - x_1;
int distance_y = y_2 - y_1;

	int distance = calc_dist(distance_x, distance_y);
printf("%d", distance);




	return 0;
}
